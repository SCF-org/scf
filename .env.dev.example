# SCF Development Environment Variables
# Copy this file to .env.dev and fill in your actual values
# Use with: npx scf deploy --env dev

# AWS Credentials for Development
# AWS_ACCESS_KEY_ID=your-dev-access-key-id
# AWS_SECRET_ACCESS_KEY=your-dev-secret-access-key

# Development Configuration
APP_NAME=my-app-dev
AWS_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=my-app-bucket-dev

# CloudFront (usually disabled for dev)
CLOUDFRONT_ENABLED=false

# Development-specific settings
# DEBUG=true
