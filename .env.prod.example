# SCF Production Environment Variables
# Copy this file to .env.prod and fill in your actual values
# Use with: npx scf deploy --env prod

# AWS Credentials for Production
# AWS_ACCESS_KEY_ID=your-prod-access-key-id
# AWS_SECRET_ACCESS_KEY=your-prod-secret-access-key

# Production Configuration
APP_NAME=my-app-prod
AWS_REGION=us-east-1

# S3 Configuration
S3_BUCKET_NAME=my-app-bucket-prod

# CloudFront Configuration
CLOUDFRONT_ENABLED=true
CLOUDFRONT_PRICE_CLASS=PriceClass_All

# Custom Domain (production)
CLOUDFRONT_DOMAIN=www.example.com
ACM_CERTIFICATE_ARN=arn:aws:acm:us-east-1:123456789012:certificate/your-prod-cert-id

# Route53 Hosted Zone
ROUTE53_HOSTED_ZONE_ID=Z1234567890ABC
